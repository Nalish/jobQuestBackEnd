    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/*","api/*"
        
      ]
    },
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}